<Window x:Class="LauncherUpdater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Launcher Updater" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Height="80" Width="525">
        <ProgressBar x:Name="progressBar1" HorizontalAlignment="Left" Height="35" Margin="10,10,0,0" VerticalAlignment="Top" Width="505"/>
        <Label x:Name="StatusLabel" Content="Ready" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="505"/>

    </Grid>
</Window>
